{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": [2, "never"],
        "indent": [2, 2],
        "react/jsx-no-undef": 1,
        "react/jsx-uses-vars": 1,
        "react/no-typos": 0,
        "react/react-in-jsx-scope": 1,
        "no-console": 0,
        "spaced-comment": 0,
        "prefer-const": 0,
        "react/self-closing-comp": 0,
        "no-inline-comments": 0,
        "no-unused-vars": 0,
        "react/no-unused-state": 0
    }
}